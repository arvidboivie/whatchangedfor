{
  "name": "parser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/parser/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "{workspaceRoot}/dist/apps/parser",
        "tsConfig": "{projectRoot}/tsconfig.json",
        "main": "{projectRoot}/src/main.ts",
        "webpackConfig": "{projectRoot}/webpack.config.js",
        "isolatedConfig": true,
        "generatePackageJson": true,
        "optimization": true,
        "sourceMap": true,
        "target": "node",
        "compiler": "tsc"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "parser:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "parser:build:development"
        },
        "production": {
          "buildTarget": "parser:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/parser/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/parser/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-functions": {
      "command": "sam build && sam local start-api",
      "options": {
        "cwd": "apps/parser"
      }
    },
    "deploy-functions": {
      "command": "sam build && sam deploy --guided",
      "options": {
        "cwd": "apps/parser"
      }
    }
  },
  "tags": []
}
